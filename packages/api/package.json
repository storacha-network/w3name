{
  "name": "@w3name/api",
  "version": "0.1.0",
  "description": "w3name api",
  "author": "",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/web3-storage/w3name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web3-storage/w3name.git"
  },
  "bugs": {
    "url": "https://github.com/web3-storage/w3name/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "files": [
    "src",
    "dist/src",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "scripts": {
    "build": "node buildworkermodule.mjs; aegir build",
    "test": "NODE_OPTIONS=--experimental-vm-modules aegir test --target node",
    "lint": "aegir lint",
    "dev": "miniflare --live-reload"
  },
  "dependencies": {
    "@google-cloud/precise-date": "^3.0.0",
    "ipns": "^1.0.1",
    "itty-router": "^2.6.1",
    "libp2p-crypto": "^0.21.2",
    "p-defer": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.13.0",
    "@types/mocha": "^9.1.1",
    "aegir": "^37.3.0",
    "miniflare": "^2.5.1"
  },
  "directories": {
    "test": "test"
  },
  "module": "dist/index.mjs",
  "private": true
}
